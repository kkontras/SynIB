      "fc_inner": 64,
      "dropout": 0.1,
      "clip_grad": false,
      "bias_infusion": {
        "method": false
      },
    "multi_loss": {
      "multi_supervised_w": {
        "combined": 1,
        "c": 0,
        "g": 0
      }
    }
    },
    "load_ongoing": false,
    "save_dir": "unimodal_audio_{}.pth.tar"
  }
